    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/mysql": "^2.15.21",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.40.0",
    "husky": "^8.0.3",
    "knex-types": "^0.5.0",
